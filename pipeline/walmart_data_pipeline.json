{
	"name": "walmart_data_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy_92i",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "walmartdatacontainer//walmart.csv"
					},
					{
						"name": "Destination",
						"value": "dbo.SalesData"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Store",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Store",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "Date",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "SaleDate",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "Weekly_Sales",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Weekly_Sales",
									"type": "Decimal",
									"physicalType": "decimal"
								}
							},
							{
								"source": {
									"name": "Holiday_Flag",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Holiday_Flag",
									"type": "Boolean",
									"physicalType": "bit"
								}
							},
							{
								"source": {
									"name": "Temperature",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Temperature",
									"type": "Decimal",
									"physicalType": "decimal"
								}
							},
							{
								"source": {
									"name": "Fuel_Price",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Fuel_Price",
									"type": "Decimal",
									"physicalType": "decimal"
								}
							},
							{
								"source": {
									"name": "CPI",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "CPI",
									"type": "Decimal",
									"physicalType": "decimal"
								}
							},
							{
								"source": {
									"name": "Unemployment",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Unemployment",
									"type": "Decimal",
									"physicalType": "decimal"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_92i",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_92i",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}